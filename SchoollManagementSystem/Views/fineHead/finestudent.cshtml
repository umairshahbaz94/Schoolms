@model IEnumerable<SMS.Entities.finelist>

@{
    ViewBag.Title = "finestudent";
    Layout = "~/Views/Shared/_Layout_New.cshtml";
}

<h2>Student-Fine-List</h2>


<table class="table table table-bordered" id="myTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RollNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.TotalFine)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.status)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.RollNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TotalFine)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.status)
                </td>

                <td>
                    <a href="~/reportsform/FinePrint.aspx?id=@item.Stdid" class="btn btn-success"><span class="fa fa-print"> Print-Fine </span></a>

                </td>
            </tr>
        }

    </tbody>
</table>
@section scripts
{

    <script>
      $(document).ready(function () {
          $('#myTable').DataTable();
      });</script>
}